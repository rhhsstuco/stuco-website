		"@types/node": "^24.3.0",
		"gh-pages": "^6.3.0",
		"sass": "^1.92.0",
		"svelte": "^5.38.6",
		"svelte-check": "^4.3.1",
		"ts-node": "^10.9.1",
		"tslib": "^2.6.2",
		"vite": "^7.1.4",
		"vite-imagetools": "^5.1.2"
	},
	"type": "module",
	"dependencies": {
		"@google-cloud/local-auth": "^3.0.1",
		"dotenv": "^17.2.2",
		"encoding": "^0.1.13",

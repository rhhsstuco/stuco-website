		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"ts-node": "^10.9.1",
		"tslib": "^2.6.2",
		"vite": "^5.4.7",
		"vite-imagetools": "^5.0.9"
	},
	"type": "module",
	"dependencies": {
		"@google-cloud/local-auth": "^2.1.0",
